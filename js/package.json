{
    "name": "enochian-js",
    "version": "0.0.1-beta.0",
    "description": "An ergonmic LLM programming library",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "ci": "pnpm build && pnpm check-format && pnpm check-exports",
        "format": "biome check --write --no-errors-on-unmatched",
        "check-format": "biome check --no-errors-on-unmatched",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "test": "node --import @swc-node/register/esm-register examples/_test.ts"
    },
    "keywords": ["LLM", "prompting"],
    "files": ["dist"],
    "author": "Colin Zhao",
    "repository": {
        "type": "git",
        "url": "https://github.com/zolinthecow/enochian-js.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.16.4",
        "@types/node": "^22.7.4",
        "typescript": "^5.6.3",
        "vitest": "^2.1.2"
    },
    "packageManager": "pnpm@9.11.0",
    "dependencies": {
        "enochian-studio": "workspace:*",
        "openai": "^4.67.3",
        "ulid": "^2.3.0",
        "zod": "^3.23.8"
    }
}
